<div class="container">
  <div class="container--row">
    <%= render 'card_transaction' %>
  </div>
  <div class="container--row">
    <%= render 'estimate' %>
  </div>
  <div class="container--row">
    <div class="comps-container">
      <div class="comparable-list">
        <div id='first-tr'><%= render 'card_transaction' %></div>
        <% @comparables.each do |comparable| %>
          <%= render 'comparable_transaction', comparable: comparable %>
        <% end %>
      </div>
      <div class="comparable-map">
        <%= render 'comparable_map' %>
      </div>
    </div>
  </div>
</div>
